#include <stdio.h>
int product(int x, int y)
{
    int product;
    product = x*y;
    return product;
}

int main(void)
{   int a,b,p;
    printf("Enter the value of a: ");
    scanf("%d",&a);
    printf("Enter the value of b: ");
    scanf("%d",&b);
    p = product(a,b);
    printf("The product of %d and %d is %d.",a,b,p);
    printf("\n");
    printf("%d * %d = %d",a,b,p);
    return 0;
}
